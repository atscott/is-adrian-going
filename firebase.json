{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "private, max-age=0, no-cache"
        }
      ]
    }]
  },
}
