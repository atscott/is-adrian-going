{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "service-worker.js",
      "headers": [
        {
          "key": "Cache-control",
          "value": "max-age=0"
        }
      ]
    }]
  },
}
